{
    "name": "tombrain/phpunit-mock-db",
    "type": "library",
    "description": "Database Abstraction Layer mocking helpers for PHPUnit",
    "keywords": ["phpunit", "database", "mock"],
    "homepage": "https://github.com/czukowski/phpunit-mock-db",
    "license": "MIT",
    "authors": [
        {
            "name": "Korney Czukowski",
            "homepage": "http://xn----0tbl7bn.com"
        }
    ],
    "support": {
        "issues": "https://github.com/czukowski/phpunit-mock-db/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tombrain/phpunit-sql.git"
        }  	    
    ],
    "require": {
        "tombrain/phpunit-sql": "dev-phpunit-9",
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Cz\\PHPUnit\\MockDB\\": "classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cz\\PHPUnit\\MockDB\\": "tests"
        }
    }
}
